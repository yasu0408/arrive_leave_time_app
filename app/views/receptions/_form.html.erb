<%= form_with model: @reception, local: true do |form| %>
  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>
  <div>
    <%= form.label :arrive %>
    <%= form.time_select :arrive %>
  </div>
  <div>
    <%= form.label :leave %>
    <%= form.time_select :leave %>
  </div>
  <div>
    <%= form.submit button_value %>
  </div>
<% end %>
